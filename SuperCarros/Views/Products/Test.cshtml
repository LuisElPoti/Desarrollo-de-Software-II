@model IEnumerable<SuperCarros.Product>

@{
    ViewBag.Title = "Test";
}

<h2>Test</h2>

<div class="row">
    @foreach (var item in Model)
            {
                <div class="col-lg-3">
                    <img src="~/Content/image/@(string.IsNullOrEmpty(item.Picture) ? "default.png" : item.Picture)" style="height:200px; width:200px" />
                </div>
            }
            
    
</div>
